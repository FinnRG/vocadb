{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"disabledUsers": {
			"type": "boolean"
		},
		"filter": {
			"type": "string"
		},
		"groupId": {
			"enum": [
				"Admin",
				"Limited",
				"Moderator",
				"Nothing",
				"Regular",
				"Trusted"
			],
			"type": "string"
		},
		"knowsLanguage": {
			"type": "string"
		},
		"onlyVerifiedArtists": {
			"type": "boolean"
		},
		"page": {
			"type": "number"
		},
		"pageSize": {
			"type": "number"
		},
		"sort": {
			"enum": ["Group", "Name", "RegisterDate"],
			"type": "string"
		}
	},
	"type": "object"
}
